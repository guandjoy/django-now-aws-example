({
	"name": "django-now-aws",
	"version": 2,
	"public": true,
	"builds": [
		{
			"src": "app.py",
			"use": "@now/python",
			"config": { "maxLambdaSize": "15mb" }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "app.py"
		}
	]
},
{
	"version": 2,
	"name": "django-now-aws",
	"builds": [
		{
			"src": "django_now_aws/wsgi.py",
			"use": "@ardnt/now-python-wsgi",
			"config": { "maxLambdaSize": "15mb" }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "django_now_aws/wsgi.py"
		}
	]
},
{
	"version": 2,
	"name": "django-now-aws",
	"builds": [
		{
			"src": "index.py",
			"use": "@contextualist/python-wsgi",
			"config": { "maxLambdaSize": "60mb" }
		}
	],
	"routes": [{ "src": "/.*", "dest": "/" }]
})
